@using Console.Plugins
@inject PluginManager _pluginmanager

@{
    ViewData["Title"] = "Reports";
    Layout = "_Layout_NonSpa";
}

@{
    foreach (Plugin plugin in _pluginmanager.Plugins.Values)
    {
        if (plugin.Reports.Count > 0)
        {
            @await Html.PartialAsync("PluginReportListPartial", plugin);
        }        
    }
}